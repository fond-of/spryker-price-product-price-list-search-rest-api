{
  "name": "fond-of-spryker/price-product-price-list-search-rest-api",
  "description": "Price product price list search rest api module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=7.1",
    "spryker/glue-application": "^1.0.0",
    "fond-of-spryker/price-product-price-list-page-search": "dev-feature/prepare-for-php8"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "fond-of-codeception/spryker": "^1.0",
    "php-coveralls/php-coveralls": "^2.1",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/"
    }
  }
}
